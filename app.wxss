/* 其他样式 */
.cell-group {
  margin-top: var(--md) !important;
}

.bg-white {
  background-color: #fff !important;
}
.bg-primary{
  background-color: var(--primary-color);
}
.tab.active{
  --line-width:1em;
  --line-height:4px;
  font-weight: 500;
  position: relative;
  cursor: pointer;
  color: #000;
}
.tab.active::after{
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translate(-50%,100%);
  width: var(--line-width);
  height: var(--line-height);
  border-radius: var(--line-height);
  background-color: var(--primary-color);
}